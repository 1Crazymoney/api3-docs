{
  "name": "compare-vuepress",
  "version": "1.0.0",
  "description": "Comparision of VuePress, Docusaurus and GitBook.",
  "main": "index.js",
  "repository": "https://github.com/wkande/compare-vuepress",
  "author": "Warren Anderson",
  "license": "MIT",
  "scripts": {
    "test:links": "for file in $(find ./docs -path ./node_modules -prune -false -o -name \"*.md\"); do markdown-link-check --verbose --config .markdown-link-check.config.json \"$file\" || exit 1; done;",
    "docs:dev": "NODE_OPTIONS='--max-old-space-size=4096' vuepress dev docs",
    "docs:build": "vuepress build docs",
    "bump": "node -e 'require(\"./docs/.vuepress/lib/versioning.js\").generate()'",
    "merge:md": "cat /docs/master/protocols/request-response/client.txt /docs/master/protocols/request-response/template.md > rrr.md"
  },
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^1.8.0",
    "@vuepress/plugin-medium-zoom": "^1.8.0",
    "markdown-link-check": "^3.8.6",
    "vuepress": "^1.8.0",
    "vuepress-plugin-element-tabs": "^0.2.8"
  },
  "dependencies": {}
}
